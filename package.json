    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "sharp": "^0.33.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@types/bun": "latest",
